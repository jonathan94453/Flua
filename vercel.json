{
  "version": 2,
  "routes": [
    { 
      "src": "/assets/(.*)", 
      "dest": "/src/front_end/",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    { 
      "src": "/(.*)", 
      "dest": "/src/front_end/index.html" 
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
